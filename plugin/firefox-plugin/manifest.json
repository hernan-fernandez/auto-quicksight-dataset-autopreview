{
  "manifest_version": 2,
  "name": "Auto-preview Uncheck",
  "version": "1.0",
  "description": "Unchecks the auto-preview slider on QuickSight.",
  "permissions": [
    "activeTab",
    "https://us-east-1.quicksight.aws.amazon.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://us-east-1.quicksight.aws.amazon.com/*"],
      "js": ["uncheckSlider.js"],
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "16": "icon16.png"
  }
}


